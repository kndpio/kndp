{
  "root": "packages/kndp/backstage/packages/backend",
  "version": 2,
  "role": "backend",
  "sourceRoot": "packages/kndp/backstage/packages/app/src",
  "projectType": "application",
  "tags": ["scope:kndp"],
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["backstage-cli package start --role backend"]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["backstage-cli package build --role backend"]
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["backstage-cli package lint --role backend"]
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["backstage-cli package test --role backend"]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["backstage-cli package clean --role backend"]
      }
    },
    "build-image": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["docker build ../.. -f Dockerfile --tag backstage"]
      }
    }
  }
}
