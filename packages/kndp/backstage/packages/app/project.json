{
    "root": "packages/kndp/backstage/packages/app",
    "version": 2,
    "role": "frontend",
    "sourceRoot": "packages/kndp/backstage/packages/app/src",
    "main": "packages/kndp/backstage/packages/app/src/index.tsx",
    "projectType": "application",
    "tags": ["scope:kndp"],
    "targets": {
      "start": {
        "executor": "nx:run-commands",
        "options": {
          "commands": ["backstage-cli package start --role frontend"]
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "commands": ["backstage-cli package build --role frontend"]
        }
      },
      "lint": {
        "executor": "nx:run-commands",
        "options": {
          "commands": ["backstage-cli package lint --role frontend"]
        }
      },
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "commands": ["backstage-cli package test --role frontend"]
        }
      },
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "commands": ["backstage-cli package clean --role frontend"]
        }
      },
      "build-image": {
        "executor": "nx:run-commands",
        "options": {
          "commands": ["docker build ../.. -f Dockerfile --tag backstage"]
        }
      }
    }
  }
